<!DOCTYPE html>
<html lang='en'>

<head>
	<meta charset="UTF-8">
	<link rel="stylesheet" href="css/bootstrap.min.css">
	<link rel="stylesheet" href="css/login.css">
	<title>WINEPICKER登陆系统</title>
</head>
<% include ./includes/head_common.ejs %>

	<body>
		<% include ./includes/nav.ejs %>
			<div class="main">
				<div class="top">
					<h1>登陆</h1>
				</div>

				<!--logo的放置-->
				<div class="img">
					<img src="./logo/logo.png" class="mylogo"alt="">

				</div>

				<!--表单套用bootstrap -->
				<div class="mid">

					<div class="col-md-6">

						<div class="form-group row">
							<div class="col-md-3 text-right">
								<label for="inputEmail">Email：</label>
							</div>
							<div class="col-md-5 p-0">
								<input type="email" class="form-control" id="inputEmail" autofocus />
							</div>
						</div>

						<div class="form-group row">
							<div class="col-md-3 text-right">
								<label for="inputPassword">密码：</label>
							</div>
							<div class="col-md-5 p-0">
								<input type="password" class="form-control" id="inputPassword" />
							</div>
						</div>
						<div class="form-group row">
							<div class="col-md-3 text-right">
							</div>
							<div class="col-md-5 p-0">
								<button class="btn btn-primary" id="loginorregisterbtn">登录</button>
							</div>
						</div>
					</div>
				</div>
			</div>
			<script type="text/javascript" src="js/login.js"></script>
	</body>
</html>